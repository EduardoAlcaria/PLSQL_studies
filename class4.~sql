create table EAL_pais_tab ( --- feito
       cod_pais varchar2(255) primary key,
       desc_pais varchar2(255),
       cod_area_fone varchar2(255)
      
);





create table EAL_estado_tab ( --- feito
       cod_estado varchar2(255) primary key,
       cod_pais varchar2(255),
       desc_estado varchar2(255),
       cod_area_fone varchar2(255),
       foreign key (cod_pais) references EAL_pais_tab(cod_pais)

);

create table EAL_area_tab( --- feito
  cod_area varchar2(255) primary key,
  desc_area varchar(255)

);

create table EAL_cidade_tab( --- feito
       cod_cidade varchar2(255) primary key,
       cod_estado varchar2(255),      
       cod_pais varchar2(255),
       cod_area varchar2(255),
       desc_cidaade varchar2(255),
       
       
       foreign key (cod_estado) references EAL_estado_tab(cod_estado),
       foreign key (cod_pais) references EAL_pais_tab(cod_pais),
       foreign key (cod_area) references EAL_area_tab(cod_area)
);





create table EAL_cliente_tab( --- feito
       cod_cliente varchar2(255) primary key,
       cod_cidade varchar2(255),
       cod_estado varchar2(255),
       cod_pais varchar2(255),
       cliente_name varchar2(255) not null,
       cpf varchar2 (255) not null,
       rg varchar2 (255) not null,
       ativo varchar2 (255) not null,
       foreign key (cod_cidade) references EAL_cidade_tab(cod_cidade),
       foreign key (cod_estado) references EAL_estado_tab(cod_estado),
       foreign key (cod_pais) references EAL_pais_tab(cod_pais)
);


create table EAL_empresa_tab( --- feito
       cod_emp_id varchar2(255) primary key,
       cod_cidade varchar2(255),
       razao_social varchar2(255),
       endereco varchar2(255),
       cep varchar2(255),
       cnpj varchar2(255),
       complemento varchar2(255),
       foreign key (cod_cidade) references EAL_cidade_tab(cod_cidade)
);



create table EAL_notafiscal_taab( --- feito
       fiscal_note number(25) primary key,
       serialNF varchar2(255),
       cFop varchar2(255),
       valorTotal number(25)

);




create table EAL_produto_tab( --- feito
       codigo_produto varchar2(255) primary key,
       descPrduto varchar2(255),
       preco number(20)
);



create table EAL_notafiscalitem_tab(  --- feito
      item number(20) primary key,
      notafiscal number(25),
      codigoProduto varchar2(255),    
      qtyVenda number(20),
      preco number(20),
      icmsPerc number(20),
      valorIcms number(20),
      valorIpi number(20),
      valorTotalTem number(20),
      foreign key (notafiscal) references EAL_notafiscal_taab(fiscal_note),
      foreign key (codigoProduto) references EAL_produto_tab(codigo_produto)
);


create table EAL_almoxarifado_tab( --- feito
      codigAlmox varchar2(255) primary key,
      cod_emp_id varchar2(255),
      descAlmox varchar2(255),
      
      foreign key (cod_emp_id) references EAL_empresa_tab(cod_emp_id)
);



create table EAL_estoque_tab ( --- feito
       qtyReservada number(20),
       codigAlmox varchar2(255),
       qtyEmEstoque number(20),
       codigo_produto varchar2(255),
       foreign key (codigAlmox) references EAL_almoxarifado_tab(codigAlmox),
       foreign key (codigo_produto) references EAL_produto_tab(codigo_produto)
);




create view EAL_pais as
select * from EAL_pais_tab order by cod_pais;


create view EAL_estado as
select * from EAL_estado_tab order by cod_estado;


create view EAL_area as
select * from EAL_area_tab order by cod_area;


create view EAL_cidade as
select * from EAL_cidade_tab order by cod_cidade asc;


create view EAL_cliente as
select * from EAL_cliente_tab order by cod_cliente asc;

create view EAL_empresa as 
select * from EAL_empresa_tab order by cod_emp_id asc;

create EAL_notafiscal  as 
select * from EAL_notafiscal_taab order by  fiscal_note asc;

create view EAL_produto as 
select * from EAL_produto_tab order by codigo_produto asc;

create view EAL_notafiscalitem as
select * from EAL_notafiscalitem_tab oder by item asc;

create view  EAL_almoxarifado as 
select * from  EAL_almoxarifado_tab order by codigAlmox asc;

create view  EAL_estoque as
select * from  EAL_estoque_tab order by qtyReservada asc;

select * from EAL_estoque;

select * from EAL_pais;

select * from EAL_notafiscalitem;






    













